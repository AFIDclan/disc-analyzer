mpirun -np 4 snappyHexMesh -overwrite -parallel
reconstructParMesh -constant


surfaceTransformPoints -rotate '((0 0 1) (0 1 0) 10)' constant/triSurface/disc.stl constant/triSurface/disc_rotated.stl
mv constant/triSurface/disc_rotated.stl constant/triSurface/disc.stl
surfaceFeatureExtract

surfaceTransformPoints -translate '(1.05 0 0)' constant/triSurface/disc.stl constant/triSurface/disc_transformed.stl
mv constant/triSurface/disc_transformed.stl constant/triSurface/disc.stl
surfaceFeatureExtract
snappyHexMesh -overwrite
