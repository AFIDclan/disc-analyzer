(3.0 0.75 -0.75) // 2
(0 0.75 -0.75) // 3
(0 -0.75 0.75) // 4
(3.0 -0.75 0.75) // 5
(3.0 0.75 0.75) // 6
(0 0.75 0.75) // 7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (200 50 50) simpleGrading (1 1 1)  // x:200 cells, y/z:50 each
);

edges
(
);

boundary
(
    inlet
    {
        type patch;
        faces
        (
            (0 3 7 4)  // x=0
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (1 2 6 5)  // x=4.2
        );
    }
    yMin
    {
        type slip;
        faces
        (
            (0 1 5 4)  // y=-1.05
        );
    }
    yMax
    {
        type slip;
        faces
        (
            (3 2 6 7)  // y=1.05
        );
    }
    zMin
    {
        type symmetryPlane;
        faces
        (
            (0 1 2 3)  // z=-1.05
        );
    }
    zMax
    {
        type symmetryPlane;
        faces
        (
            (4 5 6 7)  // z=1.05
        );
    }
);

